{
  "name": "@omni-stove/ui",
  "version": "2025.05.1",
  "main": "node_modules/expo/AppEntry.js",
  "module": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "react-native": "dist/commonjs/index.js",
  "source": "src/index.ts",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omni-stove/ui.git"
  },
  "keywords": [
    "react-native",
    "react-component",
    "ui-library",
    "material-design"
  ],
  "scripts": {
    "preinstall": "npx -y only-allow npm",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .rnstorybook",
    "storybook": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --android",
    "plop": "plop",
    "format": "biome check --write .",
    "build": "bob build",
    "build-check": "tsc --noEmit",
    "test": "vitest run",
    "knip": "knip",
    "lint": "npx run-p lint:*",
    "lint:script": "biome check .",
    "lint:spell": "cspell . --no-progress"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.2",
    "@gorhom/portal": "^1.0.14",
    "@material/material-color-utilities": "^0.3.0",
    "@pchmn/expo-material3-theme": "^1.3.2",
    "@react-native-community/slider": "4.5.6",
    "@react-native-vector-icons/material-icons": "^12.0.0",
    "@tanstack/react-table": "^8.21.3",
    "papaparse": "^5.5.3",
    "react-native-paper": "^5.12.0",
    "react-native-paper-dates": "^0.22.42",
    "react-native-paper-tabs": "^0.11.4",
    "zod": "^3.25.23",
    "react-native-reanimated": "~3.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.25.4",
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.6",
    "@expo/metro-runtime": "~5.0.4",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@storybook/addon-actions": "^8.6.12",
    "@storybook/addon-controls": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-ondevice-actions": "^8.6.2",
    "@storybook/addon-ondevice-backgrounds": "^8.6.2",
    "@storybook/addon-ondevice-controls": "^8.6.2",
    "@storybook/addon-ondevice-notes": "^8.6.2",
    "@storybook/core": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-native": "^8.6.2",
    "@storybook/react-native-web-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/papaparse": "^5.3.16",
    "@types/react": "~19.0.10",
    "@vitejs/plugin-react": "^4.5.0",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "cross-env": "^7.0.3",
    "cspell": "^9.0.2",
    "expo": "53.0.11",
    "knip": "^5.58.1",
    "npm-run-all": "^4.1.5",
    "plop": "^4.0.1",
    "react-native": "0.79.3",
    "react-native-builder-bob": "^0.40.11",
    "react-native-web": "^0.20.0",
    "storybook": "^8.6.12",
    "ts-morph": "^26.0.0",
    "typescript": "~5.8.3",
    "vite": "^6.0.11",
    "vite-plugin-babel": "^1.3.1",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=2.0.0",
    "react": ">=18.0.0 <=19.0.0",
    "react-dom": ">=18.0.0 <=19.0.0",
    "react-native": ">=0.79.0",
    "react-native-gesture-handler": ">=2.14.0",
    "react-native-reanimated": ">=3.6.0",
    "react-native-safe-area-context": ">=4.8.0",
    "react-native-svg": ">=15.0.0",
    "react-native-vector-icons": ">=10.0.0",
    "react-native-web": ">=0.20.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    }
  },
  "resolutions": {
    "react-docgen-typescript": "2.2.2",
    "react-native-web": "^0.20.0"
  },
  "overrides": {
    "react-docgen-typescript": "2.2.2",
    "react-native-web": "^0.20.0"
  },
  "pnpm": {
    "overrides": {
      "react-docgen-typescript": "2.2.2",
      "react-native-web": "^0.20.0"
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "exclude": "**/*.stories.{ts,tsx,js,jsx}",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
